
object HelloWorld

  abstract class List
  case class Nil() extends List
  case class Cons(h: Int(32), t: List) extends List

  fn length(l: List): Int(32) = {
    l match {
      case Nil() => 0
      case Cons(_, t) => 1 + length(t)
    }
  }

  val len: Int(32) = length(
	Cons(11, Cons(13, Nil()))
  )

  if(len != 2) {
	error("Uh oh! Length should be 2!")
  } else {
	Std.printString("All good.")
  }

end HelloWorld

